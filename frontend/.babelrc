{
  "presets": [
    ["@babel/preset-env", { "useBuiltIns": "usage" }],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    // "react-css-modules",
    "@babel/proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    [
      "react-intl",
      {
        "messagesDir": "./public/messages/"
      }
    ]
  ]
}
